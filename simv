#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x58e3a3cb14b0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x58e3a3cb1640 .scope module, "tb_pc" "tb_pc" 3 4;
 .timescale -9 -12;
v0x58e3a3cc6620_0 .var "clk", 0 0;
v0x58e3a3cc66e0_0 .net "current_pc", 31 0, v0x58e3a3cc62e0_0;  1 drivers
v0x58e3a3cc67b0_0 .var "next_pc", 31 0;
v0x58e3a3cc68b0_0 .var "reset", 0 0;
S_0x58e3a3c77490 .scope module, "uut" "pc" 3 13, 4 2 0, S_0x58e3a3cb1640;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 32 "next_pc";
    .port_info 3 /OUTPUT 32 "current_pc";
v0x58e3a3c77700_0 .net "clk", 0 0, v0x58e3a3cc6620_0;  1 drivers
v0x58e3a3cc62e0_0 .var "current_pc", 31 0;
v0x58e3a3cc63c0_0 .net "next_pc", 31 0, v0x58e3a3cc67b0_0;  1 drivers
v0x58e3a3cc64b0_0 .net "reset", 0 0, v0x58e3a3cc68b0_0;  1 drivers
E_0x58e3a3c8b640 .event posedge, v0x58e3a3cc64b0_0, v0x58e3a3c77700_0;
    .scope S_0x58e3a3c77490;
T_0 ;
    %wait E_0x58e3a3c8b640;
    %load/vec4 v0x58e3a3cc64b0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x58e3a3cc62e0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x58e3a3cc63c0_0;
    %assign/vec4 v0x58e3a3cc62e0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x58e3a3cb1640;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x58e3a3cc6620_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x58e3a3cb1640;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x58e3a3cc6620_0;
    %inv;
    %store/vec4 v0x58e3a3cc6620_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x58e3a3cb1640;
T_3 ;
    %vpi_call/w 3 27 "$dumpfile", "dump.vcd" {0 0 0};
    %vpi_call/w 3 28 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x58e3a3cb1640 {0 0 0};
    %vpi_call/w 3 32 "$display", "Time\011 reset next_pc    current_pc" {0 0 0};
    %vpi_call/w 3 33 "$monitor", "%0dns\011   %b    %h    %h", $time, v0x58e3a3cc68b0_0, v0x58e3a3cc67b0_0, v0x58e3a3cc66e0_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x58e3a3cc68b0_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x58e3a3cc67b0_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x58e3a3cc68b0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 4, 0, 32;
    %store/vec4 v0x58e3a3cc67b0_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 8, 0, 32;
    %store/vec4 v0x58e3a3cc67b0_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 160, 0, 32;
    %store/vec4 v0x58e3a3cc67b0_0, 0, 32;
    %delay 10000, 0;
    %delay 10000, 0;
    %vpi_call/w 3 55 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
