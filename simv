    %cmp/ne;
    %jmp/0xz  T_0.0, 6;
    %vpi_call/w 3 62 "$display", "ERROR: time=%0t READ1[%0d]=0x%08h (exp=0x%08h)", $time, v0x570f4a4d66d0_0, v0x570f4a4f9b30_0, v0x570f4a4f7b30_0 {0 0 0};
